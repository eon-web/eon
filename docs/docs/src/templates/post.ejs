<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= post.attributes.title %> | <%=posts.config.blogName%></title>
    <link rel="stylesheet" href="/index.css" />
    <link rel="stylesheet" href="/docs/assets/prism.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/katex@0.12.0/dist/katex.min.css" integrity="sha384-AfEj0r4/OFrOo5t7NnNe46zW/tFgW6x/bCJG8FqQCEo3+Aro6EYUG4+cU+KJWu/X" crossorigin="anonymous">
    <%const marked = require('marked')%>
    <%const html = require('html-format')%>
</head>

<body>
    <header>
        <div class="left">
            <img src="/docs/assets/logo.svg" alt="Logo" id="logo" />
            <nav>
                <% posts.config.navLinks.forEach(function (link) { %>
                <a href="<%= link.href %>"><%= link.text %></a>
                <% }); %>
            </nav>
        </div>
    </header>
    <aside>
        <% for ( let post_ of posts.sort((a, b) => a.attributes.number - b.attributes.number)) { %>
            <a href="/docs/<%= post_.path %>"><%= post_.attributes.title %></a>
        <% } %>
    </aside>
    <main>
        <h1 class="title"><%= post.attributes.title %></h1>
        <%- post.body %>
    </main>
</body>

</html>